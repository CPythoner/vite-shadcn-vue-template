{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:frontend\" ",
    "dev:frontend": "vite",
    "dev:server": "cd server && python -m uvicorn main:app --host 0.0.0.0 --port 3000 --reload",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "init-db": "ts-node --project tsconfig.scripts.json scripts/init-db.ts"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.453.0",
    "radix-vue": "^1.9.7",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "vue": "^3.5.10",
    "vue-router": "^4.5.0",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^22.7.6",
    "@types/sqlite3": "^3.1.11",
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite": "^5.4.8",
    "vue-tsc": "^2.1.6",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "concurrently": "^8.2.2"
  }
}
